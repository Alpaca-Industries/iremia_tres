{
  "CONFIG_VERSION_DONT_TOUCH_THIS": 3,
  "_comment": "Before changing anything, see https://github.com/Patbox/StyledChat#configuration",
  "text_formatting": {
    "legacy_formatting": false,
    "parse_links": true,
    "parse_mentions": false,
    "markdown": true,
    "respect_colors_client_setting": true
  },
  "auto_completion": {
    "tags": false,
    "tag_aliases": false,
    "emoticons": false
  },
  "default": {
    "display_name": "%luckperms:prefix% ${default}",
    "message_formats": {
      "chat": "<gr #ffffff #c7c7c7>${player}></gr> ${message}",
      "joined_the_game": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><lang:multiplayer.player.joined:'${player}'></gr>",
      "joined_after_name_change": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><lang:multiplayer.player.joined.renamed:'${player}':'${old_name}'></gr>",
      "joined_for_first_time": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><lang:multiplayer.player.joined:'${player}'></gr>",
      "left_game": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><lang:multiplayer.player.left:'${player}'></gr>",
      "base_death": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7>${default_message}</gr>",
      "advancement_task": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><lang:chat.type.advancement.task:'${player}':'${advancement}'> </gr>",
      "advancement_challenge": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><lang:chat.type.advancement.challenge:'${player}':'${advancement}'> </gr>",
      "advancement_goal": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><lang:chat.type.advancement.goal:'${player}':'${advancement}'> </gr>",
      "sent_team_chat": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><lang:'chat.type.team.sent':'<hover\\:\\'<lang\\:chat.type.team.hover>\\'><suggest_command\\:\\'/teammsg \\'>${team}':'${displayName}':'${message}'> </gr>",
      "received_team_chat": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><lang:'chat.type.team.text':'<hover\\:\\'<lang\\:chat.type.team.hover>\\'><suggest_command\\:\\'/teammsg \\'>${team}':'${displayName}':'${message}'> </gr>",
      "sent_private_message": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><italic><lang:commands.message.display.outgoing:'${receiver}':'${message}'> </gr>",
      "received_private_message": "<gr #00c7fa #00b4f5>✒ Iremia</gr> <gr #ffffff #c7c7c7><italic><lang:commands.message.display.incoming:'${sender}':'${message}'> </gr>",
      "say_command": "[${player}] <gr #ffffff #c7c7c7>${message}</gr>",
      "me_command": "<gr #ffffff #c7c7c7><lang:'chat.type.emote':'${player}':'${message}'></gr>",
      "pet_death": "<gr #00c7fa #00b4f5>✒ Iremia</gr> ${default_message}"
    },
    "link_style": "<underline><c:#7878ff>${link}",
    "mention_style": "<c:#7878ff>%player:displayname%",
    "spoiler_style": "<gray>${spoiler}",
    "spoiler_symbol": "▌",
    "formatting": {
      "dark_red": true,
      "green": true,
      "underline": true,
      "dark_green": true,
      "black": true,
      "yellow": true,
      "bold": true,
      "italic": true,
      "dark_blue": true,
      "dark_purple": true,
      "gold": true,
      "red": true,
      "aqua": true,
      "gray": true,
      "light_purple": true,
      "blue": true,
      "white": true,
      "dark_aqua": true,
      "dark_gray": true,
      "spoiler": true,
      "strikethrough": true
    },
    "emoticons": {
      "$emojibase:builtin:joypixels": "${emoji}",
      "shrug": "¯\\_(ツ)_/¯",
      "table": "(╯°□°）╯︵ ┻━┻",
      "sword": "🗡",
      "bow": "🏹",
      "trident": "🔱",
      "rod": "🎣",
      "potion": "🧪",
      "shears": "✂",
      "bucket": "🪣",
      "bell": "🔔",
      "item": "[%player:equipment_slot mainhand%]",
      "pos": "%player:pos_x% %player:pos_y% %player:pos_z%"
    },
    "custom_message_types": {}
  },
  "styles": []
}